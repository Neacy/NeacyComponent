<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/id_AAAAA"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="===============点击跳转A Component===============" />


    <TextView
        android:id="@+id/id_kotlin_click"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="============点击跳转Kotlin Component=============" />


    <TextView
        android:id="@+id/id_a"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="点击读取B Component " />


    <TextView
        android:id="@+id/id_b"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="点击读取App Component" />


    <TextView
        android:id="@+id/id_kotlin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="点击读取Kotlin Component" />

</LinearLayout>
